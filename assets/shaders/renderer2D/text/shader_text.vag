#version 330 core
layout (location = 0) in float vertex; // ingore it
out vec2 TexCoords;

uniform mat4 vertexData;
uniform mat4 projection;

void main()
{
    vec4 amogus=vertexData[gl_VertexID];
    gl_Position = projection * vec4(vertexData[gl_VertexID].xy, 0.0, 1.0);
    TexCoords = vertexData[gl_VertexID].zw;
}  